{
  "metadata": [
    {
      "src": [
        {
          "files": [ "Library/Nfield.SDK.csproj" ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "src": "../"
        }
      ],
      "dest": "code_sdk"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "code_sdk/*.yml", "toc.yml" ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "Default template documentation",
      "_appFooter":  "Â© 2012 - 2018 NIPO",
      "_appLogoPath": "images/nfield-manager.png",
      "_appFaviconPath": "images/favicon.ico", 
      "_disableContribution":  true
    },
    "dest": "pre-deploy-doc",
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ]
  }
}