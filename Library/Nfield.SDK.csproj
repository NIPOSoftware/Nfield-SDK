<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>NIPO</Authors>
    <Company>NIPO</Company>
    <Copyright>Copyright © NIPO 2018</Copyright>
    <Description>NIPO Software Nfield SDK for Windows</Description>
    <PackageLicenseUrl>http://www.gnu.org/licenses</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/NIPOSoftware/Nfield-SDK</PackageProjectUrl>
    <RepositoryUrl>https://github.com/NIPOSoftware/Nfield-SDK</RepositoryUrl>
    <PackageIconUrl>https://raw.githubusercontent.com/NIPOSoftware/Nfield-SDK/master/icon-nfield.png</PackageIconUrl>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageTags>nfield marketresearch capi cawi</PackageTags>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>..\Nfield.SDK.snk</AssemblyOriginatorKeyFile>
    <DelaySign>false</DelaySign>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.AspNet.WebApi.Client" Version="5.2.7" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.1" />
    <PackageReference Include="Nfield.Quota" Version="2.0.55492-alpha" />
    <PackageReference Include="System.Net.Http" Version="4.3.4" />
  </ItemGroup>
</Project>
